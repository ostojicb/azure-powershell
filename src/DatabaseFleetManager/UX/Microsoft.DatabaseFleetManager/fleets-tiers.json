      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.databasefleetmanager/get-azdatabasefleetmanagerfleettier"
        },
        "parameterSets": [
          {
            "parameters": [
              "-FleetName <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String[]>]",
              "-TierName <String>"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Gets a tier resource.",
          "parameters": [
            {
              "name": "-FleetName",
              "value": "[Path.fleetName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
